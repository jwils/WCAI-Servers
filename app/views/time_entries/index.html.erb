<h1>Listing time_entries</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Start time</th>
    <th>End time</th>
    <th>Timesheet</th>
    <th>Project</th>
    <th>Comment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_entries.each do |time_entry| %>
  <tr>
    <td><%= time_entry.date %></td>
    <td><%= time_entry.start_time %></td>
    <td><%= time_entry.end_time %></td>
    <td><%= time_entry.timesheet %></td>
    <td><%= time_entry.project %></td>
    <td><%= time_entry.comment %></td>
    <td><%= link_to 'Show', time_entry %></td>
    <td><%= link_to 'Edit', edit_time_entry_path(time_entry) %></td>
    <td><%= link_to 'Destroy', time_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Time entry', new_time_entry_path %>
